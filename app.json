{
  "expo": {
    "name": "RigSnap",
    "slug": "rigsnap",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      // "supportsTablet": true,
      "bundleIdentifier": "com.alexchern.rigsnap",
      "buildNumber": "24",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSCameraUsageDescription": "RigSnap uses the camera to capture photos of vehicle issues and documentation within service requests.",
        "NSLocationWhenInUseUsageDescription": "RigSnap uses your location to show nearby service providers, calculate routes, and display job locations while you use the app."
      }
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": ["expo-router", "expo-font", "expo-web-browser"],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "5dae1e10-3c70-467a-b325-786d99764f10"
      }
    },
    "android": {
      "package": "com.alexchern.rigsnap"
    },
    "owner": "saqlainch12"
  }
}

//com.axcher.rigsnap

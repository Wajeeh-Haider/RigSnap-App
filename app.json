{
  "expo": {
    "name": "RigSnap",
    "slug": "rigsnap",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "bundleIdentifier": "com.alexchern.rigsnap",
      "buildNumber": "24",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSCameraUsageDescription": "RigSnap uses the camera to capture photos of vehicle issues and documentation within service requests.",
        "NSLocationWhenInUseUsageDescription": "RigSnap uses your location to show nearby service providers, calculate routes, and display job locations while you use the app."
      }
    },
    "android": {
      "package": "com.alexchern.rigsnap",
      "permissions": ["CAMERA", "ACCESS_FINE_LOCATION", "INTERNET"]
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": ["expo-router", "expo-font", "expo-web-browser"],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "0e96c672-5ce2-4215-b363-584fe13554ce"
      }
    },
    "owner": "muaz628"
  }
}
